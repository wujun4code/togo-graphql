{
  "name": "togo-graphql",
  "version": "1.0.0",
  "description": "togo graphql",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma-import --force && npx prisma format && npx prisma migrate dev",
    "generate": "npx prisma generate",
    "build": "npm run generate && npm run migrate:deploy",
    "compile": "tsc",
    "dev": "nodemon",
    "start-dev": "npm run compile && node --inspect ./dist/index.js ",
    "start": "npm run compile && node ./dist/index.js"
  },
  "keywords": [],
  "author": "Jun Wu",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@apollo/datasource-rest": "^6.2.2",
    "@apollo/server": "^4.10.0",
    "@apollo/server-plugin-response-cache": "^4.1.3",
    "@graphql-tools/schema": "^10.0.3",
    "@prisma/client": "^5.12.1",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "fastq": "^1.17.1",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "isbot": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "prisma-import": "^1.0.5",
    "ws": "^8.16.0"
  },
  "prisma": {
    "import": {
      "schemas": "./prisma/schemas/*",
      "output": "./prisma/schema.prisma"
    }
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.19",
    "nodemon": "^3.1.0",
    "prisma": "^5.12.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20"
  }
}
